<%=listArray.length%>
<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
<style>
table {
  border-collapse: collapse;
  border-spacing: 0;
  width: 100%;
  border: 1px solid #ddd;
}

th, td {
  text-align: left;
  padding: 16px;
}

tr:nth-child(even) {
  background-color: #f2f2f2;
}
</style>
</head>
<body>
<table>
  <tr>
    <th>No</th>
    <th>제목</th>
    <th>작성자</th>
    <th>등록일</th>
    <th>조회수</th>
  </tr>
  <%for(var i=0;i<listArray.length;i++){%>    
  <% var json = listArray[i];%>    
  <tr>
    <td>26</td>
    <td>
        <a href="/board/detail?board_id=<%=json.board_id%>"><%=json.title%> [ <%=json.cnt%> ] </a>
    </td>
    <td><%=json.writer%></td>
    <td><%=json.regdate%></td>
    <td><%=json.hit%></td>
  </tr>
  <%}%>
</table>

<button onClick="location.href='/board/write.html'">글등록</button>

</body>
</html>
